<Window x:Class="k_means_cluster.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="k-means-cluster" Height="350" Width="525">
    <Window.Background>
        <ImageBrush ImageSource="/Properties/k-means.png" Stretch="UniformToFill" ></ImageBrush>
    </Window.Background>
    <Grid>
        <DatePicker HorizontalAlignment="Left" Margin="63,136,0,0" VerticalAlignment="Top"/>
        <DatePicker HorizontalAlignment="Left" Margin="225,136,0,0" VerticalAlignment="Top"/>
        <Button Content="Upload file" HorizontalAlignment="Left" Margin="63,61,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock HorizontalAlignment="Left" Margin="165,64,0,0" TextWrapping="Wrap" Text="Upload a .csv file. Ex. AMRN, MSFT, GOOG, ..." VerticalAlignment="Top" FontWeight="Bold" Foreground="#FFFDFCFC"/>
        <Button Content="Get Clusters" HorizontalAlignment="Left" Margin="63,205,0,0" VerticalAlignment="Top" Width="86"/>
        <ComboBox HorizontalAlignment="Left" Margin="351,115,0,0" VerticalAlignment="Top" Width="120" />
        <ComboBox HorizontalAlignment="Left" Margin="351,163,0,0" VerticalAlignment="Top" Width="120" />
        <Label Content="X coordinate" HorizontalAlignment="Left" Margin="351,85,0,0" VerticalAlignment="Top"/>
        <Label Content="Y coordinate" HorizontalAlignment="Left" Margin="351,137,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
